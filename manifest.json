{
  "name": "YouTube shortcuts",
  "version": "0.1",
  "manifest_version": 2,
  "description": "YouTube shortcuts",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "YouTube shortcuts",
    "default_popup": "popup.html"
  }
}
